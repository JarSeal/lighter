{
  "name": "lighter",
  "version": "2.0.0",
  "description": "A light weight TS UI framework, with Component, Router, and State handling.",
  "keywords": [
    "UI",
    "Framework",
    "Light",
    "Component",
    "Router",
    "State"
  ],
  "main": "src/Lighter/index.ts",
  "repository": "https://github.com/JarSeal/lighter.git",
  "author": "JarSeal",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite serve --port 8080",
    "lint": "eslint . --ext .ts",
    "test": "jest --verbose --runInBand --detectOpenHandles",
    "build": "tsc && vite build",
    "buildPackageJson": "node buildPackageJson"
  },
  "dependencies": {
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "eslint": "8.57.0",
    "eslint-config-import": "0.13.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "prettier": "2.8.6",
    "typescript": "5.3.3",
    "vite": "4.2.1"
  },
  "engines": {
    "node": ">=16.13.2",
    "yarn": ">=1.22.15"
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
